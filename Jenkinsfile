                echo 'git clone completed'
            }
        }
		stage('compile') {
            steps {
                echo 'compile completed'
            }
        }
		stage('unit testing') {
            steps {
                echo 'unit testing completed'
            }
        }
		stage('package') {
            steps {
                echo 'package'
            }
        }
		stage('deploy') {
            steps {
                echo 'deploy'
            }
        }
    }
}
